AWS_ENDPOINT_URL=http://localstack.localstack:4566
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
POLARIS_AUTHENTICATION_TOKEN_BROKER_TYPE=rsa-key-pair
POLARIS_AUTHENTICATION_TOKEN_BROKER_RSA_KEY_PAIR_PUBLIC_KEY_FILE=/polaris/credentials/public-key
POLARIS_AUTHENTICATION_TOKEN_BROKER_RSA_KEY_PAIR_PRIVATE_KEY_FILE=/polaris/credentials/private-key
POLARIS_REALM_CONTEXT_REALMS={{ plf_realm }}
POLARIS_REALM_CONTEXT_HEADER_NAME=Polaris-Realm
POLARIS_REALM_CONTEXT_REQUIRE_HEADER=false
POLARIS_PERSISTENCE_TYPE=eclipse-link
POLARIS_PERSISTENCE_ECLIPSELINK_CONFIGURATION_FILE=/polaris/config/persistence.xml
POLARIS_PERSISTENCE_ECLIPSELINK_PERSISTENCE_UNIT={{ plf_jpa_persistence_unit_name }}
JAVA_OPTS_APPEND=-Dquarkus.log.level=DEBUG -Dquarkus.log.console.level=DEBUG -Dquarkus.log.category."org.apache.polaris".level=DEBUG -Dquarkus.log.category."org.apache.polaris".min-level=TRACE